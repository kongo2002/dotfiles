! Xft settings {{{
xft.dpi:                        96
xft.antialias:                  true
xft.hinting:                    true
xft.hintstyle:                  hintslight
xft.rgba:                       rgb
xft.lcdfilter:                  lcddefault
! }}}

! General settings {{{
*background:                    #202020
*foreground:                    #e0e0e0
! }}}

! Xterm settings {{{
xterm*termName:                 xterm-256color

! allow external programs to modify the xterm window
xterm*allowWindowOps:           true
xterm*geometry:                 85x35
xterm*boldMode:                 false
!xterm*colorBD:                  #eeeeee
!xterm*colorBDMode:              true

! font settings
!xterm*font:                 -*-proggycleanszcp-*-*-*-*-*-*-*-96-*-70-*-*
!xterm*boldFont:             -*-proggycleanszcp-*-*-*-*-*-*-*-96-*-70-*-*
!xterm*font:                 -*-andale mono-medium-r-*-*-*-120-*-*-*-*-*-15
!xterm*boldFont:             -*-andale mono-medium-r-*-*-*-120-*-*-*-*-*-15
!xterm*font:                  -*-terminus-medium-*-*-*-17-*-*-*-*-*-*-15
!xterm*boldFont:              -*-terminus-medium-*-*-*-17-*-*-*-*-*-*-15
xterm*faceName:             Inconsolata
xterm*faceSize:             12
!xterm*boldFont:             xft:Dejavu Sans Mono:pixelsize=14.0:antialias=true:hinting=true
! }}}

! URxvt settings {{{
URxvt*termName:                 rxvt-256color

!URxvt*colorBD:                  orange
!URxvt*colorIT:                  cyan
URxvt*colorUL:                  #ff9922
URxvt*jumpScroll:               True
URxvt*scrollstyle:              plain
URxvt*loginShell:               True
URxvt*scrollBar:                False
URxvt*scrollTtyOutput:          False
URxvt*scrollTtyKeypress:        True
URxvt*scrollWithBuffer:         False
URxvt*saveLines:                50000
URxvt*internalBorder:           0
URxvt*externalBorder:           0
!URxvt*cursorColor:              grey50
URxvt*cursorBlink:              True
URxvt*imLocale:                 de_DE.UTF-8
URxvt*geometry:                 85x35

! open urls with middle mouse click
URxvt.perl-ext-common:          default,matcher
URxvt.matcher.button:           2
URxvt.matcher.pattern.1:        \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.urlLauncher:              firefox
!URxvt.keysym.C-Delete:          perl:matcher

! screen scrollbackbuffer
!URxvt*secondaryScroll:          true

! font settings
URxvt*font:                 xft:Inconsolata:pixelsize=16.0:antialias=true:hinting=true
URxvt*boldFont:             xft:Inconsolata:pixelsize=16.0:antialias=true:hinting=true
URxvt*italicFont:           xft:Inconsolata:pixelsize=16.0:antialias=true:hinting=true
URxvt*bolditalicFont:       xft:Inconsolata:pixelsize=16.0:antialias=true:hinting=true
!URxvt*font:                 xft:Terminus:pixelsize=14.0:antialias=true:hinting=true
!URxvt*boldFont:             xft:Terminus:pixelsize=14.0:antialias=true:hinting=true
!URxvt*italicFont:           xft:Terminus:pixelsize=14.0:antialias=true:hinting=true
!URxvt*bolditalicFont:       xft:Terminus:pixelsize=14.0:antialias=true:hinting=true
!URxvt*font:                 xft:Dejavu Sans Mono:pixelsize=14.0:antialias=true:hinting=true
!URxvt*boldFont:             xft:Dejavu Sans Mono:pixelsize=14.0:antialias=true:hinting=true
!URxvt*italicFont:           xft:Dejavu Sans Mono:pixelsize=14.0:antialias=true:hinting=true
!URxvt*bolditalicFont:       xft:Dejavu Sans Mono:pixelsize=14.0:antialias=true:hinting=true

! }}}

